{
    "title": {
        "primary": "Αλγοριθμική Επιχειρησιακή Έρευνα",
        "secondary": "Τρίτη Εργασία"
    },
    "authors": [
        "Σιώρος Βασίλειος",
        "Ανδρινοπούλου Χριστίνα"
    ],
    "packages": [
        "alphabeta",
        "latexsym",
        "amsthm",
        "amsmath",
        "titlesec",
        "stackrel",
        "MnSymbol",
        "graphicx",
        "caption",
        "fullpage",
        "listings",
        "pxfonts",
        "color"
    ],
    "commands": {
        "\\R": "\\mathbb{R}",
        "\\N": "\\mathbb{N}",
        "\\norm": "\\left\\lVert#1\\right\\rVert",
        "\\margin": "\\hspace{4pt}",
        "\\centered": "\\begin{align*}#1\\end{align*}",
        "\\plot": "\\includegraphics{#1}\\caption{#2}",
        "\\code": "\\lstinputlisting[caption={#2}]{#1}"
    },
    "environments": {
        "rcases": {
            "begin": "\\left.\\begin{aligned}",
            "end": "\\end{aligned}\\right\\rbrace"
        },
        "matlab": {
            "begin": "\\begin{figure}[hp]\\centering\\captionsetup{justification=centering}",
            "end": "\\end{figure}"
        }
    }
}